#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Digite a quantidade de termos da sequência de Fibonacci: ";
    cin >> n;

    int a = 0, b = 1;
    cout << "Sequência de Fibonacci: ";
    for(int i = 1; i <= n; i++) {
        if(i == 1) {
            cout << a;
        } else if(i == 2) {
            cout << " " << b;
        } else {
            int c = a + b;
            cout << " " << c;
            a = b;
            b = c;
        }
    }
    cout << endl;
    return 0;
}

